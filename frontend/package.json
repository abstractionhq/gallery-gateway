{
  "name": "react-webpack-3",
  "version": "2.0.0",
  "description": "boilerplate for a webpack3 react project",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development BABEL_ENV=development node ./server.js",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "debug": "DEBUG=true npm run build -- --display-optimization-bailout",
    "lint": "eslint --ext .js --ext .jsx app",
    "validate": "npm ls"
  },
  "pre-push": [
    "lint"
  ],
  "author": "Kayla Davis",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^1.9.3",
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^4.2.3",
    "express": "^4.15.3",
    "graphql-tag": "^2.5.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-sse": "github:rit-sse/eslint",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "pre-push": "^0.1.1",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.4.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.5.1"
  }
}
