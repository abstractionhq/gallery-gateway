{
  "name": "abstraction-backend",
  "private": true,
  "version": "1.0.0",
  "main": "server.js",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "report-dir": "./coverage",
    "sourceMap": false,
    "instrument": false
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    }
  },
  "eslintIgnore": [
    "build/*",
    "coverage/*"
  ],
  "scripts": {
    "start": "npm run build && node ./build/main.js",
    "watch": "backpack",
    "build": "backpack build",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 5000 test/setup.js test/**/*.js",
    "test-file": "cross-env NODE_ENV=test nyc mocha test/setup.js",
    "keygen": "babel-node keygen",
    "sequelize": "babel-node ./node_modules/sequelize-cli/lib/sequelize",
    "migrate": "babel-node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "seed": "babel-node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "lint": "eslint **/*.js"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "archiver": "^3.1.1",
    "backpack-core": "^0.7.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.1",
    "csv-stringify": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "graphql": "^0.11.7",
    "graphql-errors": "^2.1.0",
    "graphql-tools": "^2.5.1",
    "jsonwebtoken": "^8.1.0",
    "jszip": "^3.1.5",
    "keypair": "^1.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.0",
    "moment-timezone": "^0.5.16",
    "multer": "^1.3.0",
    "mysql2": "^1.4.2",
    "nconf": "^0.8.5",
    "passport": "^0.4.0",
    "passport-saml": "^0.30.0",
    "sequelize": "^4.15.0",
    "sequelize-cli": "^3.2.0",
    "sharp": "^0.20.1",
    "umzug": "^2.1.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  }
}
